{
    "manifest_version": 3,
    "name": "hiding UT logo , FB log in ",
    "version": "0.1.0",
    "description": "hiding UT logo , FB log in ",
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/*"],
            "js": ["youtube.js"]
        },
        {
            "matches": ["https://*.facebook.com/*"],
            "js": ["facebook.js"]
        }
    ],
    
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    }
}

